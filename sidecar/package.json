{
  "name": "copilotforge-sidecar",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start:dist": "node dist/index.js",
    "doctor": "node scripts/doctor.mjs",
    "probe:tools": "node scripts/sdk-tool-count-probe.mjs",
    "check": "npm run typecheck && npm run build && npm run doctor",
    "validate:release": "npm run check"
  },
  "dependencies": {
    "@github/copilot-sdk": "latest",
    "@opentelemetry/api": "^1.9.0",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.4.0",
    "typescript": "^5.9.2"
  }
}
